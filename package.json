{
  "name": "sailorcms",
  "private": false,
  "version": "0.2.0",
  "type": "module",
  "description": "A modern headless CMS built with SvelteKit, featuring dynamic schema generation, flexible content management, and seamless database integration.",
  "keywords": [
    "svelte",
    "sveltekit",
    "cms",
    "typescript",
    "drizzle",
    "content-management"
  ],
  "author": "Jon Kristian Nilsen",
  "license": "MIT",
  "homepage": "https://jonkristian.github.io/sailorcms/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonkristian/sailorcms.git"
  },
  "bugs": {
    "url": "https://github.com/jonkristian/sailorcms/issues"
  },
  "bin": {
    "sailor": "./bin/sailor.js"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "sailor": "bun cli/index.js"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.36.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.43.6",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.6.1",
    "@types/xmldom": "^0.1.34",
    "bits-ui": "^2.11.4",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rollup-plugin-visualizer": "^6.0.3",
    "svelte": "^5.39.7",
    "svelte-check": "^4.3.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "terser": "^5.44.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vaul-svelte": "^1.0.0-next.7",
    "vite": "^6.3.6",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.899.0",
    "@aws-sdk/s3-request-presigner": "^3.899.0",
    "@floating-ui/dom": "^1.7.4",
    "@internationalized/date": "^3.9.0",
    "@libsql/client": "^0.15.15",
    "@lucide/svelte": "^0.515.0",
    "@sveltejs/adapter-node": "^5.3.2",
    "@tailwindcss/vite": "^4.1.13",
    "@tiptap/core": "^3.6.2",
    "@tiptap/extension-image": "^3.6.2",
    "@tiptap/extension-link": "^3.6.2",
    "@tiptap/extension-text-align": "^3.6.2",
    "@tiptap/extension-underline": "^3.6.2",
    "@tiptap/pm": "^3.6.2",
    "@tiptap/starter-kit": "^3.6.2",
    "@types/pg": "^8.15.5",
    "@xmldom/xmldom": "^0.9.8",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.24",
    "clsx": "^2.1.1",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.5",
    "fs-extra": "^11.3.2",
    "highlight.js": "^11.11.1",
    "mode-watcher": "^1.1.0",
